#! /bin/bash

# determine the OS
# use a switch statement to determine command

## ISS00: determine what the OS is

GROUP=$(egrep -i "svadmin |carpadmin |sudo" /etc/group)
echo $GROUP

USERS=$(grep -i "GROUP" /etc/group)

OS=$(uname -o)

case $OS in
    GNU/Linux)
	echo "OS is $OS"
#	reset user passwd
	;;
#   Solaris)
#	;;
#   RHEL)
#	;;
#   SuSe)
#	;;
    *)
	echo "$OS is unsupported at this time!"
	exit 1
	;;
esac

exit 0
